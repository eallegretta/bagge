RewriteEngine On

RewriteBase /

# CULTURE: Default
RewriteRule		^/inicio\.aspx									/Default.aspx

RewriteRule		^/clientes/listado\.aspx$						/CustomerList.aspx
RewriteRule		^/clientes/alta\.aspx$							/CustomerEditor.aspx
RewriteRule		^/clientes/editar/(\d+)\.aspx$  				/CustomerEditor.aspx?Id=$1
RewriteRule		^/clientes/ver\(\d+)\.aspx$						/CustomerEditor.aspx?Id=$1&Action=View

RewriteRule		^/proveedores/listado\.aspx$					/ProviderList.aspx
RewriteRule		^/proveedores/alta\.aspx$						/ProviderEditor.aspx
RewriteRule		^/proveedores/editar/(\d+)\.aspx$				/ProviderEditor.aspx?Id=$1
RewriteRule		^/proveedores/ver\(\d+)\.aspx$					/ProviderEditor.aspx?Id=$1&Action=View

RewriteRule		^/insumos/listado\.aspx$						/ProductList.aspx
RewriteRule		^/insumos/alta\.aspx$							/ProductEditor.aspx
RewriteRule		^/insumos/editar/(\d+)\.aspx$					/ProductEditor.aspx?Id=$1
RewriteRule		^/insumos/ver\(\d+)\.aspx$						/ProductEditor.aspx?Id=$1&Action=View

RewriteRule   	^/tickets/listado\.aspx$						/TicketList.aspx
RewriteRule   	^/tickets/alta\.aspx$							/TicketEditor.aspx
RewriteRule   	^/tickets/editar/(\d+)\.aspx$					/TicketEditor.aspx?Id=$1
RewriteRule   	^/tickets/ver\(\d+)\.aspx$						/TicketEditor.aspx?Id=$1&Action=View

RewriteRule   	^/empleados/listado\.aspx$						/EmployeeList.aspx
RewriteRule   	^/empleados/alta\.aspx$							/EmployeeEditor.aspx
RewriteRule   	^/empleados/editar/(\d+)\.aspx$					/EmployeeEditor.aspx?Id=$1
RewriteRule   	^/empleados/ver\(\d+)\.aspx$					/EmployeeEditor.aspx?Id=$1&Action=View

RewriteRule		^/reportes/clientes_por_abono\.aspx$			/Reports/CustomersBySubscription.aspx
RewriteRule		^/reportes/clientes_por_ticket\.aspx$			/Reports/CustomersByTicket.aspx
RewriteRule		^/reportes/clientes_pendientes_de_pago\.aspx$	/Reports/CustomersWithPendingPayment.aspx
RewriteRule		^/reportes/productos_por_proveedor\.aspx$		/Reports/ProductsByProvider.aspx
RewriteRule		^/reportes/productos_consumidos\.aspx$			/Reports/ProductsConsumed.aspx
RewriteRule		^/reportes/tecnicos_por_ticket\.aspx$			/Reports/TechniciansByTicket.aspx
RewriteRule		^/reportes/roles_por_usuario\.aspx$				/Reports/RolesByUser.aspx

RewriteRule		^/alertas/configurar.aspx						/AlertConfiguration.aspx

RewriteRule   	^/roles/listado\.aspx$							/RoleList.aspx
RewriteRule   	^/roles/alta\.aspx$								/RoleEditor.aspx
RewriteRule   	^/roles/editar/(\d+)\.aspx$						/RoleEditor.aspx?Id=$1
RewriteRule   	^/roles/ver\(\d+)\.aspx$						/RoleEditor.aspx?Id=$1&Action=View

RewriteRule		^/salir\.aspx									/Logout.ashx

# ENDCULTURE